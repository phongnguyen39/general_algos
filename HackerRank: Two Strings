// Two Strings

// compare two strings to see if they share commmon strings (as small as one char)
// output of yes or no

function twoStrings(s1,s2){
  let dict = {}
  // use split() to parse out characters
  // store one of the strings in a dictionary (reference)
  // compare second string split() to the dictionary

  let s1Split = s1.split('')
  let s2Split = s2.split('')

  for(let i = 0; i < s1Split.length; i++){
    dict[s1Split[i]] = 1;
  }
  // console.log(dict)

  for(let j = 0; j < s2Split.length; j++){
    if(dict[s2Split[j]]){
      console.log("yes")
      return 'YES'
    } 
    
  }
  return 'NO'
  
}
// twoStrings('hello','world') // yes
twoStrings('hi','world') // no
