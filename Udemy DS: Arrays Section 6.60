const strings = ['a','b','c','d']
// 4*4 = 16 bytes of storage

// lookups up 'c'  O(1)
strings[2] 

// push operation O(1)
strings.push('e');
console.log(strings)

// pop operation O(1)
strings.pop()
console.log(strings)

// unshift operation O(n), items in the array had to be moved over
strings.unshift('x')
console.log(strings)

// splice operation O(n), removes or replaces items in an array (technically O(n/2) depending on place of replacement ore removal)
strings.splice(2,0,'alien')  // 2 - starting index, 0 - nothing to remove, 'alien' -add element
console.log(strings)

